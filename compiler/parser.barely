        variable boolean_pointer_data = NodeBooleanData.*cast(nodes_append_data(nodes, NODE_BOOLEAN, get_token_location_data(tokens, whole_8.>(index_pointer))));
        NodeBooleanData.<value(boolean_pointer_data, boolean_value);

