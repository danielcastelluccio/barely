    if =(tokens_get_id(tokens, index), TOKEN_OPEN_PARENTHESIS) {
        variable inner = 0_integer_8;

        while |(>(inner, 0_integer_8), =(tokens_get_id(tokens, index), TOKEN_OPEN_PARENTHESIS)) {
