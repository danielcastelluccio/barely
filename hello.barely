function main() {
	variable test1: integer = 4;
	test4(ptr(test1));

	variable test: long = test2(5_4);
	test3(test);
};

function test4(thing: *integer) {
	@print_integer(thing);
};

function test3(thing: long) {
	@print_integer(@get_first_long(thing));
};

function test2(thing: long): long {
	return thing;
};

function print(string: *) {
	@print(string, @length(string));
};
