function main() {
	variable test: Test;
	Test<-value(ptr(test), 4);
	Test<-value2(ptr(test), 9);
	@print_integer(Test->value(ptr(test)));
	@print_integer(Test->value2(ptr(test)));
};

function print(string: *) {
	@print(string, @length(string));
};

structure Test {
	value: integer;
	value2: integer;
};
